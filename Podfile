# Uncomment this line to define a global platform for your project
 platform :ios, '10.0'

target 'Widget' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!


  # Pods for Widget
  pod "QRCode"
  pod 'SwiftyJSON'
  pod 'Alamofire', '~> 4.0'
end

target 'Widget-Demo' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Widget-Demo
  pod "QRCode"

  pod 'Alamofire', '~> 4.0'
